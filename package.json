		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@tailwindcss/vite": "^4.1.18",
		"@vitest/coverage-v8": "^4.0.18",
		"eslint": "^9.39.2",
		"eslint-plugin-svelte": "^3.15.0",
		"globals": "^17.3.0",
		"svelte": "^5.51.0",
		"svelte-check": "^4.4.0",
		"tailwindcss": "^4.1.18",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.55.0",
		"vite": "^7.3.1",
		"vitest": "^4.0.18",
		"wrangler": "^4.65.0"
